  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vite-pwa/astro": "^0.4.3",
