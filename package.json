{
  "name": "glossary",
  "type": "module",
  "version": "0.1.0",
  "description": "My personal website built with Astro, Qwik, Tailwind CSS, MDX, and the Github API",
  "private": true,
  "repository": "git@github.com:princemuel/glossary.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.25.1",
    "@fontsource-variable/inconsolata": "^5.2.5",
    "@fontsource-variable/inter": "^5.2.5",
    "@fontsource-variable/lora": "^5.2.5",
    "@sveltejs/adapter-vercel": "^5.7.1",
    "@sveltejs/kit": "^2.20.8",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.5",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-svelte": "^3.5.1",
    "globals": "^16.0.0",
    "lefthook": "^1.11.12",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.6",
    "tailwindcss": "^4.1.5",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.4"
  }
}
